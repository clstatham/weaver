@startup
system Setup {
    var a = 0
    var b = 0
    loop (a < 100) && (b < 100) {
        a += 1
        b += a

        spawn(ABC {
            a = a
            b = b
            c = a + b
        })
    }
}

func threeplus(value: int) {
    print(3 + value)
}

@update
system Update {
    threeplus(5)
}
